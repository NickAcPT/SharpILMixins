      "type": "parameter",
      "defaultValue": "SharpILMixins.Template",
	  "fileRename": "sharpilmixin_project",
      "replaces": "SharpILMixins.Template",
      "dataType": "string"
    },
    "target": {
      "type": "parameter",
      "defaultValue": "MixinTarget.dll",
      "replaces": "MixinTarget.dll",
      "dataType": "string"
    },
    "mixin": {
      "type": "parameter",
      "defaultValue": "MixinTarget.dll",
      "replaces": "MixinTarget.dll",
      "dataType": "string"
    }
  }
}
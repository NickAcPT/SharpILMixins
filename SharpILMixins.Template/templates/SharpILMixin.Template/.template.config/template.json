{
  "$schema": "http://json.schemastore.org/template",
  "author": "NickAc",
  "classifications": [ "Mixins", "Library", "Mixin", "SharpILMixins" ],
  "identity": "SharpILMixin",
  "name": "Mixin with SharpILMixins",
  "shortName": "sharpilmixin",
  "symbols": {
